@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

find_dependency(guanaqo)
if (@KOQKATOO_WITH_OPENMP@)
    find_dependency(OpenMP COMPONENTS CXX)
endif()
if (@KOQKATOO_WITH_MKL@)
    if (NOT TARGET MKL::MKL)
        set(koqkatooCore_NOT_FOUND_MESSAGE "MKL not found. Please use find_package(MKL) with the appropriate options before find_package(koqkatoo).")
        set(koqkatooCore_FOUND False)
        return()
    endif()
elseif(@KOQKATOO_WITH_OPENBLAS@)
    find_dependency(OpenBLAS)
else()
    find_dependency(BLAS)
    find_dependency(LAPACK)
endif()
include("${CMAKE_CURRENT_LIST_DIR}/koqkatooCoreTargets.cmake")

check_required_components(koqkatooCore)
