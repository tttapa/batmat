@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

find_dependency(guanaqo COMPONENTS BLAS)
if (@BATMAT_WITH_OPENMP@)
    find_dependency(OpenMP COMPONENTS CXX)
endif()
if (@BATMAT_WITH_MKL@)
    if (NOT TARGET MKL::MKL)
        set(batmatCore_NOT_FOUND_MESSAGE "MKL not found. Please use find_package(MKL) with the appropriate options before find_package(batmat).")
        set(batmatCore_FOUND False)
        return()
    endif()
elseif(@BATMAT_WITH_OPENBLAS@)
    find_dependency(OpenBLAS)
else()
    find_dependency(BLAS)
    find_dependency(LAPACK)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/batmatCoreTargets.cmake")

check_required_components(batmatCore)
